<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>

  <nlog
    internalLogFile="D:\Logs\TimberMill\Dev\ConsoleAppTester\nlog.log"
    internalLogLevel="Trace"
    throwsExceptions="true"
    xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <variable name="defaultLayout" value="${longdate} ${level:uppercase=true:padding=5} | ${message} | ${exception:format=tostring}"/>
    <targets>
      <target name="Trace"        xsi:type="Trace"          layout="${defaultLayout}" />
      <target name="ColorConsole" xsi:type="ColoredConsole" layout="${defaultLayout}"/>

      <target xsi:type="AsyncWrapper"
          name="AsyncLogReceiver"
          queueLimit="100"
          batchSize="5"
          overflowAction="Block">
      <!--<target name="result" type="BufferingWrapper" bufferSize="100" flushTimeout="1000" slidingTimeout="false">-->

        <target xsi:type="LogReceiverService"
            name="TimberMill"
            endpointAddress="http://localhost:6565/Data/LogReceiverService.svc"
            useBinaryEncoding="false"
            clientId="ConsoleAppTester">
          
            <parameter name="cs" layout="${callsite}" />
            <parameter name="message" layout="${message}foobar" />
            <parameter name="category" layout="production" />  
          
        </target>
      </target>
    </targets>
    <rules>
      <logger name="*" minlevel="Trace" writeTo="Trace,ColorConsole,TimberMill"/>
    </rules>
  </nlog>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
